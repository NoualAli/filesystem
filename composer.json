{
    "name": "nldev/filesystem",
    "description": "API to simplify manage files",
    "license": "MIT",
    "authors": [{
        "name": "NLDev",
        "email": "noualdev@gmail.com",
        "homepage": "https://github.com/NoualAli"
    }],
    "homepage": "https://github.com/NoualAli/filesystem",
    "keywords": ["Laravel", "FileSystem"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "NLDev\\FileSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NLDev\\FileSystem\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NLDev\\FileSystem\\FileSystemServiceProvider"
            ],
            "aliases": {
                "FileSystem": "NLDev\\FileSystem\\Facades\\FileSystem"
            }
        }
    }
}